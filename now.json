{
  "version": 2,
  "alias": ["molefrog.com", "www.molefrog.com"],
  "builds": [
    {
      "src": "out/**",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/stateful-animations(.*)",
      "status": 302,
      "headers": {
        "Location": "https://molefrog.github.io/stateful-animations/"
      }
    },
    {
      "src": "/pidnn-talk(.*)",
      "status": 302,
      "headers": {
        "Location": "https://molefrog.github.io/pidnn-talk/"
      }
    },
    {
      "src": "/rye(.*)",
      "status": 302,
      "headers": {
        "Location": "https://molefrog.github.io/rye/"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/out/$1"
    }
  ]
}
